{
  "root": "dist/sk-portfolio/browser",
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/**/*.@(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/index.html": {
      "Cache-Control": "no-cache"
    }
  }
}
